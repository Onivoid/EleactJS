{
  "name": "React-Webpack-Electron_Boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "presetup": "npm i",
    "setup": "npm run w:prod",
    "w:prod": "webpack --mode production",
    "postw:prod": "npm run electron:view",
    "w:dev": "webpack --mode development --watch true",
    "electron:view": "electron .",
    "electron:build": "electron-builder"
  },
  "build": {
    "productName": "Reactron",
    "appId": "com.reactron.app",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "**/*",
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "path": "^0.12.7",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  }
}
